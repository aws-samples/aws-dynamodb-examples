{
  "name": "online-shopping-store-frontend",
  "version": "0.1.0",
  "description": "React TypeScript frontend for the online shopping store e-commerce platform",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "axios": "^1.11.0",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.2.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "node scripts/build-csp.js && craco start",
    "build": "craco build",
    "build:prod": "NODE_ENV=production node scripts/build-csp.js && NODE_ENV=production craco build",
    "serve": "npx serve -s build -l 3000",
    "serve:prod": "npm run build:prod && npm run serve",
    "test": "craco test",
    "eject": "react-scripts eject",
    "clean": "rm -rf build node_modules/.cache && npm cache clean --force",
    "prebuild": "npm run clean:build",
    "clean:build": "rm -rf build",
    "clean:cache": "rm -rf node_modules/.cache && npm cache clean --force",
    "clean:all": "npm run clean && rm -rf node_modules",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "security:check": "npm audit && npm outdated",
    "security:update": "npm update && npm audit fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/dompurify": "^3.0.5",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "serve": "^14.2.4"
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31",
    "webpack-dev-server": "^4.15.1",
    "svgo": "^3.0.0",
    "css-select": "^5.1.0"
  }
}